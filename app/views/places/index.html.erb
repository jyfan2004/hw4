<div class="row">
  <div class="col-md-3 ms-4">
    <h1>Places</h1>

    <ul>
      <% for place in @places %>
        <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
      <% end %>
    </ul>

    <p>
      <a href="/places/new" class="btn btn-default">New place</a>
    </p>
  </div>
</div>